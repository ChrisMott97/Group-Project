{
  "name": "api-gateway",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "start:dev": "npm run migrate && npm run seed && nodemon ./bin/www",
    "migrate": "knex migrate:latest --knexfile database/knexfile.js",
    "seed": "knex seed:run --knexfile database/knexfile.js"
  },
  "dependencies": {
    "auth0": "^2.40.0",
    "axios": "^0.26.1",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "debug": "~2.6.9",
    "express": "^4.17.3",
    "express-oauth2-jwt-bearer": "^1.1.0",
    "express-openid-connect": "^2.7.2",
    "knex": "^1.0.3",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.6"
  }
}
