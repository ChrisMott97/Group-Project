    "express": "~4.16.1",
    "knex": "^1.0.3",
    "morgan": "~1.9.1",
    "mysql2": "^2.3.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.6"
  }
}
